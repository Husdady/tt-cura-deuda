{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@root/*": ["*"],
      "@services/*": ["src/services/*"],
      "@public/*": ["public/*"],
      "@styles/*": ["src/styles/*"],
      "@hooks/*": ["src/hooks/*"],
      "@redux/*": ["src/redux/*"],
      "@utils/*": ["src/utils/*"],
      "@containers/*": ["src/containers/*"],
      "@layouts/*": ["src/components/layouts/*"],
      "@common/*": ["src/components/common/*"],
      "@heads": ["src/components/_heads/_exports"]
    }
  }
}
